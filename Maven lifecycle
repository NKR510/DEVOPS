Life cycle clean -> [pre-clean, clean, post-clean]

Lifecycle default -> [validate, 
								initialize, generate-sources, process-sources, generate-resources, process-resources, 
								compile,
								process-classes, generate-test-sources, process-test-sources, generate-test-resources, process-test-resources, test-compile, process-test-classes,
								test, prepare-package, package, pre-integration-test, integration-test, post-integration-test, verify, 
								install, 
								deploy]
 


 Lifecycle site -> [pre-site, site, post-site, site-deploy]
